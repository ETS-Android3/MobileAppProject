<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginBottom="?attr/actionBarSize"
    android:layout_marginTop="25dp"
    android:background="#FFF1FF"
    tools:context=".Developers">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingTop="40dp">

            <TextView
                android:id="@+id/textView10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#C6AEC6"
                android:fontFamily="@font/roboto"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:text="MEET THE TEAM"
                android:textAlignment="center"
                android:textSize="20sp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/btnBackDevs"
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="16dp"
                android:layout_marginTop="6dp"
                android:layout_marginEnd="358dp"
                android:background="@drawable/ic_baseline_arrow_back_24" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="25dp"
            android:layout_marginRight="15dp">

            <ImageView
                android:id="@+id/imageView22"
                android:layout_width="179dp"
                android:layout_height="218dp"
                android:background="#A8CED5"
                android:paddingTop="30dp"
                app:srcCompat="@drawable/kyle2" />

            <ImageView
                android:id="@+id/imageView24"
                android:layout_width="177dp"
                android:layout_height="218dp"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="2dp"
                android:background="#FAA8A6"
                android:paddingTop="30dp"
                app:srcCompat="@drawable/shaina2" />

        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="15dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView21"
                android:layout_width="181dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/roboto"
                android:text="Elmer Kyle De Leon"
                android:textAlignment="center"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView30"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_weight="1"
                android:fontFamily="@font/roboto"
                android:text="Shaina Yu"
                android:textAlignment="center"
                android:textStyle="bold" />
        </LinearLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="15dp">

            <ImageView
                android:id="@+id/imageView23"
                android:layout_width="179dp"
                android:layout_height="218dp"
                android:background="#EEDCCD"
                android:paddingTop="50dp"
                app:srcCompat="@drawable/kean" />

            <ImageView
                android:id="@+id/imageView25"
                android:layout_width="177dp"
                android:layout_height="218dp"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="2dp"
                android:background="#A4DEBE"
                android:paddingTop="30dp"/>

        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="15dp"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/textView20"
                android:layout_width="181dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/roboto"
                android:text="Kean Aylward Cabel"
                android:textAlignment="center"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView29"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_weight="1"
                android:fontFamily="@font/roboto"
                android:text="Sources"
                android:textAlignment="center"
                android:textStyle="bold" />
        </LinearLayout>



    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>